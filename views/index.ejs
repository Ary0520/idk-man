               <script>
                const words = ["hello", "one", "crazy"];
                let index = 0;
                const typewriter = document.getElementById("typewriter");
                
                function typeEffect(word, callback) {
                    let i = 0;
                    typewriter.textContent = "";
                    const typingInterval = setInterval(() => {
                        if (i < word.length) {
                            typewriter.textContent += word[i];
                            i++;
                        } else {
                            clearInterval(typingInterval);
                            setTimeout(() => eraseEffect(word, callback), 1000);
                        }
                    }, 150);
                }
                
                function eraseEffect(word, callback) {
                    let i = word.length;
                    const erasingInterval = setInterval(() => {
                        if (i > 0) {
                            typewriter.textContent = word.substring(0, i - 1);
                            i--;
                        } else {
                            clearInterval(erasingInterval);
                            setTimeout(callback, 500);
                        }
                    }, 100);
                }
                
                function loopWords() {
                    typeEffect(words[index], () => {
                        index = (index + 1) % words.length;
                        loopWords();
                    });
                }
                
                loopWords();
            </script>

